{
  "overrides": [
    {
      "files": "./**/*.js",
      "options": {
        "array-bracket-spacing": ["always"],
        "object-curly-spacing": ["always"],
        "printWidth": 180,
        "tabWidth": 2,
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "semi": false,
        "useTabs": false,
        "endOfLine": "lf"
      }
    }
  ]
}
